{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
        "data_source_id": {
            "type": "integer"
        },
        "profile": {
            "type": "integer"
        },
        "name": {
            "type": "string"
        },
        "iamid": {
            "type": "string"
        },
        "userid": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "admin": {
            "type": "string"
        },
        "approved": {
            "type": "boolean"
        },
        "state": {
            "type": "string"
        },
        "systemGenerated": {
            "type": "boolean"
        },
        "lastExternalRefresh": {
            "type": "string"
        },
        "subscriptionId": {
            "type": ["null", "integer"]
        },
        "createdAt": {
            "type": "string"
        },
        "updatedAt": {
            "type": "string"
        },
        "approvals": {
            "type": "array",
            "additionalItems": true,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "properties": {
                            "requiredPermission": {
                                "type": "string"
                            },
                            "state": {
                                "type": "string"
                            },
                            "approverId": {
                                "type": "null"
                            },
                            "ownerModelId": {
                                "type": "null"
                            },
                            "approver": {
                                "type": "string"
                            },
                            "ownerModelName": {
                                "type": "null"
                            }
                        },
                        "additionalProperties": true
                    }
                ]
            }
        }
    },    
    "additionalProperties": true
}